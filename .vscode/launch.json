    "version": "0.2.0",
    "configurations": [
        {
            "name": "url-shorten",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/roadmapsh/urlshorten/cmd/url-shorten.go",
            "env": {},
        },
        {
            "name": "caching-proxy",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/roadmapsh/cachingproxy/cmd/caching-proxy.go",
            "env": {},
            "args": [
                "--port",
                "8080",
                "--origin",
                "https://www.baidu.com"
            ]
        },
        {
            "name": "broadcast-server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/roadmapsh/broadcastserver/cmd/broadcast-server.go",
            "env": {},
            "args": [
                "--port",
            ]
        }
    ]
}
